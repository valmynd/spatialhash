{
  "name": "spatialhash",
  "version": "0.0.1",
  "description": "Spatial Hash.",
  "main": "dist/spatialhash.js",
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava",
    "report-coverage": "npm run coverage && nyc report --reporter=lcov | codecov",
    "bundle": "webpack"
  },
  "ava": {
    "files": "tests/*.js",
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "modern-browsers",
        {
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valmynd/spatialhash.git"
  },
  "keywords": [
    "spatialhash"
  ],
  "author": "C. Wilhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valmynd/spatialhash/issues"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-modern-browsers": "^10.0.1",
    "codecov": "^3.0.0",
    "nyc": "^11.4.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "gl-mat4": "^1.1.4",
    "regl": "^1.3.1"
  }
}
