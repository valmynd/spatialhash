{
  "name": "spatialhash",
  "version": "0.0.1",
  "description": "Spatial Hash.",
  "main": "dist/spatialhash.js",
  "scripts": {
    "test": "npm run build && ava",
    "coverage": "npm run build && nyc ava",
    "report-coverage": "npm run coverage && nyc report --reporter=lcov | codecov",
    "build": "babel src --out-dir dist",
    "build-single": "webpack"
  },
  "ava": {
    "files": "tests/*.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/valmynd/spatialhash.git"
  },
  "keywords": [
    "spatialhash"
  ],
  "author": "C. Wilhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valmynd/spatialhash/issues"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-runtime": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.0.0",
    "nyc": "^11.4.1"
  },
  "dependencies": {
    "babel-loader": "^7.1.2"
  }
}
